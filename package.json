{
    "name": "simple-generic-object-array-search-bar-filter",
    "version": "0.0.4",
    "description": "A simple search bar filter optimized to be used with any object array",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "./lib"
    },
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.6.1",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "chai": "^4.3.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "mocha": "^9.0.3",
        "moment": "^2.29.1",
        "prettier": "^2.3.2",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    },
    "scripts": {
        "prepare": "tsc",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "lint": "eslint src test",
        "lint-fix": "eslint --fix src test",
        "format": "prettier --check src test .prettierrc.js .eslintrc.js",
        "format-fix": "prettier --write src test .prettierrc.js .eslintrc.js",
        "test-custom": "ts-node test/customTest.ts",
        "test": "mocha --require ts-node/register test/**/**/*.test.ts test/**/*.test.ts test/*.test.ts"
